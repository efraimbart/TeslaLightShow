{
    "version": 2,
    "builds": [
        {
            "src": "nuxt.config.js",
            "use": "@nuxtjs/vercel-builder@0.22.1",
            "config": {
                "serverFiles": ["common/**", "api/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/sw.js",
            "continue": true,
            "headers": {
              "Cache-Control": "public, max-age=0, must-revalidate",
              "Service-Worker-Allowed": "/"
            }
        }
    ]
}
